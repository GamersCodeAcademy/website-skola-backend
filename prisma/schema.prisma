generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String? @db.VarChar(255)
  username String  @db.VarChar(20)
  email    String  @unique(map: "email") @db.VarChar(100)
  password String  @db.Text
}
